.article
    font-size 20px
    color #333
    padding 20px
    margin-bottom 50px
    background-color #708090
    border 1px white solid
    -moz-box-shadow 0px 2px 5px #58B1B9
    -webkit-box-shadow 0px 2px 5px #58B1B9
    box-shadow 0px 0px 5px #58B1B9
    text-shadow 0 1px 1px white

.article-container
    margin auto
    text-shadow none 

.inner
    background-color #C7EDCC
    border 1px #F2F3F3 solid
    padding 20px
    word-break break-all
